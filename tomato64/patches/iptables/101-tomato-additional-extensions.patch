+		printf("--host");
+		break;
+	case IPT_WEB_RURI:
+		printf("--req");
+		break;
+	case IPT_WEB_PATH:
+		printf("--path");
+		break;
+	case IPT_WEB_QUERY:
+		printf("--query");
+		break;
+	case IPT_WEB_HORE:
+		printf("--hore");
+		break;
+	default:
+		printf("--http");
+		return;
+	}
+
+	text = info->text;
+	printf(" \"");
+	while (*text) {
+		while (*text) {
+			if (*text == '"') printf("\\\"");
+				else putc(*text, stdout);
+			++text;
+		}
+		++text;
+		if (*text == 0) break;
+		putc(' ', stdout);
+	}
+	printf("\" ");
+}
