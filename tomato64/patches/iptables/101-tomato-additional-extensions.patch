+#ifdef IPT_LIB_DIR
+#define xtables_match iptables_match
+#define xtables_register_match register_match
+#endif
+
+static void
+help(void)
+{
+    printf(
+    "IPP2P v%s options:\n"
+    " --ipp2p	Grab all known p2p packets\n"
+    " --edk		[TCP&UDP]	All known eDonkey/eMule/Overnet packets\n"
