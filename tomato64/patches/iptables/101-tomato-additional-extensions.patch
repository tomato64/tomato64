+static void
+parse_ethport(const char *s, struct xt_ethport_info *dinfo)
+{
+	unsigned int portnum;
+       
+	if (!xtables_strtoui(s, NULL, &portnum, 0, UINT8_MAX))
+		xtables_error(PARAMETER_PROBLEM,
+			   "Invalid portnum `%s'\n", s);
