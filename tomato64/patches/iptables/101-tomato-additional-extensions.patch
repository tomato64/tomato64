+
+	if ((p = malloc(strlen(s) + 2)) == NULL) {
+		xtables_error(PARAMETER_PROBLEM, "Not enough memory");
+	}
+
+	e = p;
+	while (*s) {
+		while ((*s == ' ') || (*s == '\n') || (*s == '\t')) ++s;
+		if (*s == 0) break;
+		while ((*s != 0) && (*s != ' ') && (*s != '\n') && (*s != '\t')) {
+			*e++ = *s++;
+		}
+		*e++ = 0;
+	}
+	n = (e - p);
+
+#if 0
